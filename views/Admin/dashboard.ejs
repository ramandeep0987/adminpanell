<script>
  // Pie Chart 1
  
  const gernel = "<%= Gernal %>";
  const privateCount = "<%= Private %>";
  const evening = "<%= Evening %>";
  new Chart(document.getElementById('pieChart1'), {
    type: 'pie',
    data: {
      labels: ['Gernal ', 'Private', 'Evening'],
      datasets: [{
          data: [gernel, privateCount, evening],
        backgroundColor: ['#e2b916cc', '#46b6e8', '#710e38']
      }]
    }
  });
  const FootFall = "<%= futfaal %>";

  // Pie Chart 2
  new Chart(document.getElementById('pieChart2'), {
    type: 'pie',
    data: {
      labels: [' Total FootFall',],
      datasets: [{
        data: [FootFall],
        backgroundColor: ['#98259e']
      }]
    }
  });
  const Unsatisfied = "<%= Unsatisfied %>";
  const Satisfied = "<%= Satisfied %>";

  // Pie Chart 3
  new Chart(document.getElementById('pieChart3'), {
    type: 'pie',
    data: {
      labels: ['Unsatisfied', 'Satisfied'],
      datasets: [{
        data: [Unsatisfied, Satisfied],
        backgroundColor: ['#e40505', '#38bf3f']
      }]
    }
  });
</script>



